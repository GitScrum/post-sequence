{
    "parser": "babel-eslint",
    "plugins": [
        "prettier",
        "unicorn",
        "babel",
        "markdown"
    ],
    "extends": [
        "plugin:ava/recommended",
        "plugin:unicorn/recommended",
        "xo",
        "xo-space"
    ],
    "rules": {
        "capitalized-comments": "off",
        "unicorn/prevent-abbreviations": "off"
    }
}
